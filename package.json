{
    "name": "react-typograms",
    "description": "Unofficial React bindings for Typograms",
    "version": "0.3.3",
    "main": "dist/index.js",
    "keywords": [
        "typograms",
        "react",
        "react-typograms",
        "diagram"
    ],
    "author": "Soaple",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/soaple/react-typograms.git"
    },
    "bugs": {
        "url": "https://github.com/soaple/react-typograms/issues"
    },
    "homepage": "https://github.com/soaple/react-typograms#readme",
    "peerDependencies": {
        "react": ">=18"
    },
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "tsup src/index.ts --dts --format esm,cjs",
        "publish:public": "rm -rf dist && npm install && npm run build && npm publish --access public"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/preset-env": "^7.22.10",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@types/node": "^16.18.41",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "babel-loader": "^9.1.3",
        "tsup": "^7.2.0",
        "typescript": "^4.9.5"
    }
}
